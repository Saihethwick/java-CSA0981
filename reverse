import java.util.Scanner;
public class main{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("enter the string");
        String c=s.nextLine();
        String b=" ";
        int l=c.length();
        for(int i=l-1;i>=0;i--)
        {
            b=b+c.charAt(i);
        }
        System.out.print(b);
    }
}
